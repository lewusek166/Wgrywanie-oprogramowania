    "Default"=dword:00000000

[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Shell\OnTop]
    "Default"=dword:00000001

[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Clock]
    "SHOW_CLOCK"=dword:00000000
